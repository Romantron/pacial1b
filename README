# Pruebas Unitarias con Mockito
#Se crear la clase ContactoControllerTest.java con el siguiente c√≥digo para las pruebas. 

```package com.agenda.agenda;

import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;
import org.junit.jupiter.api.extension.ExtendWith;
import org.mockito.junit.jupiter.MockitoExtension;

import java.util.List;

import static org.junit.jupiter.api.Assertions.*;

@ExtendWith(MockitoExtension.class)
class ContactoControllerTest {

    private ContactoController contactoController;

    @BeforeEach
    void setUp() {
        contactoController = new ContactoController();
    }

    @Test
    void testAgregarContacto() {
        Contacto contacto = new Contacto("Juan P√©rez", "3001234567", "juan.perez@email.com");

        String resultado = contactoController.addContacto(contacto);
        assertEquals("Contacto agregado: Juan P√©rez", resultado);
    }

    @Test
    void testAgregarContactoDuplicado() {
        Contacto contacto = new Contacto("Juan P√©rez", "3001234567", "juan.perez@email.com");
        contactoController.addContacto(contacto); // Se agrega por primera vez

        String resultado = contactoController.addContacto(contacto); // Se intenta agregar de nuevo
        assertEquals("El contacto ya existe.", resultado);
    }

    @Test
    void testObtenerContactos() {
        contactoController.addContacto(new Contacto("Juan P√©rez", "3001234567", "juan.perez@email.com"));
        contactoController.addContacto(new Contacto("Mar√≠a G√≥mez", "3019876543", "maria.gomez@email.com"));

        List<Contacto> contactos = contactoController.getContactos();
        assertEquals(2, contactos.size());
        assertEquals("Juan P√©rez", contactos.get(0).getNombre());
        assertEquals("Mar√≠a G√≥mez", contactos.get(1).getNombre());
    }
} 
```

